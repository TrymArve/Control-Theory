		{"id":"f93dfbc0be0b3e9e","type":"text","text":"# LP Solvers","x":2285,"y":-636,"width":210,"height":106,"color":"6"},
		{"id":"9557bf51cd0edb62","type":"text","text":"# Active Set Method","x":2360,"y":-230,"width":360,"height":380,"color":"6"},
